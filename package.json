{
  "name": "electron__player",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --progress --colors --hot --config ./webpack.config.js",
    "watch": "webpack-dev-server --progress --colors --hot --config ./webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arman37/electron-player"
  },
  "keywords": [
    "Electron",
    "smart",
    "player",
    "react"
  ],
  "author": "Arman Bhuiyan <arman.it37@gmail.com>",
  "license": "no-license",
  "build": {
    "productName": "Electron Player",
    "appId": "electronplayerid",
    "files": [
      "dist/",
      "node_modules/",
      "main.js",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "electron": "^1.6.11",
    "react-hot-loader": "^1.3.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "base64-js": "^1.2.0",
    "classnames": "^2.2.5",
    "drivelist": "^5.0.4",
    "electron-config": "^0.2.1",
    "immutable": "^3.8.1",
    "material-ui": "^0.16.6",
    "musicmetadata": "^2.0.5",
    "react": "^15.5.4",
    "react-autocomplete": "^1.0.0-rc2",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-sound": "^0.5.2",
    "react-tap-event-plugin": "^2.0.1",
    "reactjs-coverflow": "^1.1.0",
    "readdirp": "^2.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "soundmanager2": "^2.97.20150601-a",
    "styled-components": "^1.4.5"
  }
}
